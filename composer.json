{
    "name": "slack/hack-json-schema",
    "description": "Hack JSON Schema generator",
    "type": "library",
    "authors": [
        {
            "name": "Michael Hahn",
            "email": "mh@slack-corp.com"
        }
    ],
    "require": {
        "facebook/hack-codegen": "^4.1.0",
        "hhvm/hsl": "^4.0.1",
        "hhvm/type-assert": "^4.1.0"
    },
    "require-dev": {
        "hhvm/hhast": "^4.0.5",
        "facebook/fbexpect": "^2.3.0",
        "hhvm/hacktest": "^1.3|^2.0"
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    },
    "config": {
        "allow-plugins": {
            "hhvm/hhvm-autoload": true
        }
    }
}
